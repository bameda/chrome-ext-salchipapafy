{
    "manifest_version": 2,
    "name": "Salchipapafy",
    "version": "0.3",
    "author": "David Barragán Merino (@bameda)",
    "description": "A funny Chrome extensión to get away from buzzwords about the entrepreneur world",
    "homepage_url": "https://github.com/bameda/chrome-ext-salchipapafy",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png",
        "256": "icons/icon256.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["libs/findAndReplaceDOMText.js", "content.js"],
            "all_frames": true
        }
    ]
}
